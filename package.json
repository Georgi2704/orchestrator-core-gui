    "npm-run-all": "^4.1.1",
    "prop-types": "^15.6.0",
    "react-codemirror": "^1.0.0",
    "react-copy-to-clipboard": "^5.0.0",
    "react-highlight": "^0.10.0",
    "react-modal": "^2.3.2",
