    "scripts": {
        "analyze": "source-map-explorer build/static/js/main.*",
        "start": "react-scripts start",
        "build": "set \"INLINE_RUNTIME_CHUNK=false\" && react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "storybook": "start-storybook -p 9009 -s ./public",
        "build-storybook": "build-storybook -s ./public",
        "prettier": "prettier -c \"{**/*.{js,jsx,scss,md,ts,tsx,json},public/**/*.html}\"",
        "prettier-fix": "prettier --write \"{**/*.{js,jsx,scss,md,ts,tsx,json},public/**/*.html}\""
    },
    "importSort": {
        ".js, .jsx, .ts, .tsx": {
            "style": "module"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "git-format-staged -f 'prettier --stdin --stdin-filepath \"{}\"' '*.js' '*.jsx' '*.ts' '*.tsx' '*.scss' '*.json' '*.gql', '*.md'"
        }
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
